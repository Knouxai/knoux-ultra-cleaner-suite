# Knoux CleanMaster Ultra™ Environment Configuration
# Copy this file to .env and modify as needed

# App Configuration
VITE_APP_NAME="Knoux CleanMaster Ultra™"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENVIRONMENT="development"

# Theme Configuration
VITE_DEFAULT_THEME="dark"
VITE_GLASS_OPACITY="0.2"
VITE_NEON_INTENSITY="0.6"

# Performance Settings
VITE_AUTO_CLEAN_ENABLED="true"
VITE_AUTO_CLEAN_INTERVAL="weekly"
VITE_TURBO_MODE_AVAILABLE="true"

# Security Settings
VITE_BLACK_DIAMOND_ENABLED="true"
VITE_SECURITY_LEVEL="high"

# Analytics & Monitoring
VITE_ANALYTICS_ENABLED="false"
VITE_ERROR_REPORTING="true"

# Feature Flags
VITE_EXPERIMENTAL_FEATURES="false"
VITE_AI_FEATURES="true"
VITE_QUANTUM_OPTIMIZER="false"
